<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC
"-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="chatMapper">
	<resultMap type="Chat" id="chatroom">
		<result property="c_no" column="C_NO"/>
		<result property="c_name" column="C_NAME"/>
		<result property="c_desc" column="C_DESC"/>
		<result property="c_cdate" column="C_CDATE"/>
		<result property="c_cruid" column="C_CRUID"/>
		<result property="c_tid" column="C_TID"/>
	</resultMap>
	
	
	<select id="selectChatList" resultMap="resultChat">
		select * from tb_chat
	</select>
	
	
</mapper>
